generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
}

model Product {
  id          Int     @id @default(autoincrement())
  name        String  @db.VarChar(100)
  price       String  @db.VarChar(100)
  description String? @db.VarChar(150)

  @@map("products")
}
